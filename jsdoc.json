{
    "plugins": ["plugins/markdown"],
    "source": {
        "include": ["server", "client"],
        "includePattern": ".js",
        "excludePattern": "(node_modules/|jsdoc)"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "destination": "./jsdoc", 
        "recurse": true,         
        "template": "./template"
        }
}